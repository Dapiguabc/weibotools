{
  "name": "weibo-auto-check-in-tool",
  "version": "0.1.1",
  "description": "微博超话自动签到、自动点赞工具。",
  "license": "GNU General Public License v3.0",
  "scripts": {
    "runel": "cross-env NODE_ENV=development electron .",
    "asar": "asar pack ../asar ../app.asar",
    "start": "npm run cleanbuild && cross-env NODE_ENV=development webpack --config ./config/webpack.dev.js --watch",
    "build": "npm run cleanbuild && cross-env NODE_ENV=production webpack --config ./config/webpack.pro.js --progress",
    "cleanbuild": "rimraf ./build",
    "devdll": "cross-env NODE_ENV=development webpack --config ./config/webpack.dll.js --progress",
    "prodll": "cross-env NODE_ENV=production webpack --config ./config/webpack.dll.js --progress",
    "npmi": "npm install --production",
    "yarni": "yarn install --production=true --pure-lockfile",
    "clean": "node-modules-clean --ext \".opts|.map|.ts|.yml|.png|.dot|.jpg\" --file \"test.js\"",
    "eslint": "eslint ./src",
    "get-version": "node ./bin/get-version.js",
    "find": "node ./bin/find.js"
  },
  "main": "bin/main.js",
  "dependencies": {
    "request": "^2.87.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.54",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.54",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.54",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.54",
    "@babel/preset-flow": "^7.0.0-beta.54",
    "@babel/preset-react": "^7.0.0-beta.54",
    "asar": "^0.14.3",
    "babel-eslint": "^9.0.0-beta.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-component": "^1.1.1",
    "bundle-loader": "^0.5.6",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.5",
    "element-ui": "^2.4.4",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "indexeddb-tools": "^2.0.4",
    "js-base64": "^2.4.8",
    "mini-css-extract-plugin": "^0.4.1",
    "moment": "^2.22.2",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.6",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "node-modules-clean": "^0.1.1"
  },
  "pre-commit": {
    "run": [
      "eslint"
    ]
  }
}
